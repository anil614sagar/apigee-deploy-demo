{
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "require-jsdoc": "off",
    "max-len": "off",
    "valid-jsdoc": "off",
    "no-tabs": "off",
    "one-var": "off",
    "no-var":"off",
    "guard-for-in":"off",
    "new-cap":"off"
  }
}
